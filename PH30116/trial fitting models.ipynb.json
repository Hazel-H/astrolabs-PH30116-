   "source": [
    "#Necessary imports\n",
    "import numpy\n",
    "import pylab\n",
    "import LightCurveSimulator\n",
    "#We will be testing these fits on the lightcurve you know from the LightCurveSimu notebook.\n",
    "lc = LightCurveSimulator.ShortTransit()\n",
    "lc.add_noise(sn=500)\n",
    "import scipy.optimize\n",
    "from scipy import interpolate\n",
    "from scipy.optimize import curve_fit"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "t, flux, error = lc.getdata()\n",
    "\n",
    "flux = lc.getdata()[1]\n",
    "time = lc.getdata()[0]\n",
    "error =lc.getdata()[2]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def lightcurve_model(t, base_flux, transit_flux, t_start, t_end):\n",
    "    y=numpy.ones(len(t))\n",
    "    for i in range(0,len(t)):\n",
    "        if (t[i] > t_start) and (t[i] < t_end):\n",
    "            y[i] = transit_flux\n",
    "        else: \n",
    "            y[i] = base_flux\n",
    "    return y "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
